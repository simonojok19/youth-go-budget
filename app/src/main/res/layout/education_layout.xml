<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:layout_margin="@dimen/fab_margin"
        card_view:cardCornerRadius="5dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="region"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/region"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="district"
            android:layout_marginTop="@dimen/seperation"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/district"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="subcounty"
            android:layout_marginTop="@dimen/seperation_60"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/subcounty"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="gender"
            android:layout_marginTop="90dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/gender"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ageCategory"
            android:layout_marginTop="120dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/ageCategory"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="level_of_education"
            android:layout_marginTop="150dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/level_of_education"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="use_education"
            android:layout_marginTop="180dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/use_education"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="best_use_education_service"
            android:layout_marginTop="210dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/best_use_education_service"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="best_use_education_spec"
            android:layout_marginTop="240dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/best_use_education_spec"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="best_use_education_reason"
            android:layout_marginTop="270dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/best_use_education_reason"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="worst_use_education_service"
            android:layout_marginTop="300dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/worst_use_education_service"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="worst_use_education_spec"
            android:layout_marginTop="330dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/worst_use_education_spec"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="worst_use_education_reason"
            android:layout_marginTop="360dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/worst_use_education_reason"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="priority_use_education_service"
            android:layout_marginTop="390dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/priority_use_education_service"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="priority_use_education_spec"
            android:layout_marginTop="420dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/priority_use_education_spec"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="priority_use_education_reason"
            android:layout_marginTop="450dp"
            android:layout_marginLeft="@dimen/fab_margin"
            android:id="@+id/priority_use_education_reason"
            android:layout_marginBottom="@dimen/seperation"/>
    </androidx.cardview.widget.CardView>

</RelativeLayout>
